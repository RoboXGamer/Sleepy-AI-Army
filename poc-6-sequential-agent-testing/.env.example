# Copy this file to .env and replace the placeholder with your actual API key.
# Get an API key from Google AI Studio: https://aistudio.google.com/apikey

# Set to FALSE to use Google AI Studio (requires GOOGLE_API_KEY)
# Set to TRUE to use Google Cloud Vertex AI (requires GOOGLE_CLOUD_PROJECT and GOOGLE_CLOUD_LOCATION)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Required if GOOGLE_GENAI_USE_VERTEXAI=FALSE
GOOGLE_API_KEY=PASTE_YOUR_GOOGLE_AI_STUDIO_API_KEY_HERE

# Required if GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_CLOUD_PROJECT=YOUR_PROJECT_ID
# GOOGLE_CLOUD_LOCATION=YOUR_REGION # e.g., us-central1